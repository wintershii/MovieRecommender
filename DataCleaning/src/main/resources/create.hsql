CREATE EXTERNAL TABLE ods.ods_user_behavior_click(
  uid string,
  mid string,
  agent string
)
PARTITIONED BY (
  year string,
  month string,
  day string
)
ROW FORMAT DELIMITED
    FIELDS TERMINATED BY ','
LOCATION '/hive/external/ods/behavior/';

alter table ods.ods_user_behavior_click add partition(year='2020', month='12', day='04')
location '/hive/external/ods/behavior/year=2020/month=12/day=04';